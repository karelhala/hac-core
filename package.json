{
  "name": "hac-core",
  "version": "1.1.0",
  "private": false,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "workspaces": [
    "src/poc-code/console-dynamic-plugin-sdk",
    "src/poc-code/console-plugin-sdk"
  ],
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "build": "npm run build-sdk && NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build lint test",
    "lint": "npm-run-all lint:*",
    "-lint:js": "eslint config src",
    "-lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run deploy",
    "start": "webpack serve --config config/dev.webpack.config.js",
    "start:proxy": "PROXY=true BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:proxy:beta": "npm run build-sdk && PROXY=true BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:proxy:beta:ci": "ENVIRONMENT=ci npm run start:proxy:beta",
    "start:beta": "BETA=true npm start",
    "start:standalone": "STANDALONE=true npm start",
    "build-sdk": "npm run build --prefix src/poc-code/console-dynamic-plugin-sdk",
    "test": "exit 0",
    "verify": "npm-run-all build lint test"
  },
  "dependencies": {
    "@patternfly/patternfly": "^4.135.2",
    "@patternfly/quickstarts": "^1.3.0",
    "@patternfly/react-catalog-view-extension": "^4.12.74",
    "@patternfly/react-core": "^4.135.0",
    "@redhat-cloud-services/frontend-components": "^3.3.3",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.2",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.4",
    "ajv": "^6.12.6",
    "chalk": "^2.4.2",
    "classnames": "^2.3.1",
    "comment-json": "^4.1.1",
    "find-up": "^4.1.0",
    "lodash-es": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "5.2.0",
    "read-pkg": "^5.2.0",
    "redux": "4.1.0",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "semver": "^6.3.0",
    "webpack-virtual-modules": "^0.4.3"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.1",
    "@redhat-cloud-services/frontend-components-config": "^4.3.5",
    "@scalprum/core": "^0.0.12",
    "@types/classnames": "^2.3.1",
    "@types/jest": "21.x",
    "@types/react-redux": "7.x",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.29.0",
    "eslint-loader": "4.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.5",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.19.0",
    "ts-node": "5.x",
    "typesafe-actions": "^5.1.0",
    "typescript": "4.4.4",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "insights": {
    "appname": "hac-core"
  }
}
